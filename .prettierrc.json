{
  "semi": false,
  "tabWidth": 2,
  "singleQuote": true,
  "printWidth": 100,
  "trailingComma": "none",
  "pugSingleQuote": false,
  "pugAttributeSeparator": "none",
  "pugClassNotation": "attribute",
  "pugIdNotation": "as-is",
  "pugFramework": "vue",
  "pugBracketSameLine": "true",
  "pugWrapAttributesThreshold": 1,
  "plugins": ["@prettier/plugin-pug"]
}
