{
  "name": "@nu/shared-lib",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./css": "./dist/style.css",
    "./assets/scss/*": [
      "./dist/src/assets/scss/*/index.scss",
      "./dist/src/assets/scss/*.scss",
      "./dist/src/assets/scss/*"
    ],
    "./*": {
      "types": [
        "./src/*.ts",
        "./src/*",
        "./src/*/index.ts"
      ],
      "default": [
        "./dist/src/*.es.js",
        "./dist/src/*",
        "./dist/src/*/index.es.js"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build-watch": "vite build --watch",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.0",
    "@vue/tsconfig": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "path": "^0.12.7",
    "vue-tsc": "^1.8.6"
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.8",
    "lodash-es": "^4.17.21"
  }
}
