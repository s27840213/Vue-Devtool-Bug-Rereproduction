{
  "name": "@nu/shared-lib",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "require": "./dist/index.d.ts",
        "default": "./dist/index.d.ts"
      },
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./plugin": {
      "types": {
        "require": "./dist/plugin.d.ts",
        "default": "./dist/plugin.d.ts"
      },
      "import": "./dist/plugin.es.js",
      "require": "./dist/plugin.cjs.js"
    },
    "./types": {
      "types": {
        "require": "./dist/components.d.ts",
        "default": "./dist/components.d.ts"
      },
      "import": "./dist/types.es.js",
      "require": "./dist/types.cjs.js"
    },
    "./css": {
      "import": "./dist/plugin.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build-watch": "vite build --watch",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.0",
    "@vue/tsconfig": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "path": "^0.12.7",
    "vue-tsc": "^1.8.6"
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.8",
    "lodash-es": "^4.17.21"
  }
}
