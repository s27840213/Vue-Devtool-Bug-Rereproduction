{
  "name": "@nu/shared-lib",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "require": "./dist/index.d.ts",
        "default": "./dist/index.d.ts"
      },
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./plugin": {
      "types": {
        "require": "./dist/plugin.d.ts",
        "default": "./dist/plugin.d.ts"
      },
      "import": "./dist/plugin.es.js",
      "require": "./dist/plugin.cjs.js"
    },
    "./types": {
      "types": {
        "require": "./dist/components.d.ts",
        "default": "./dist/components.d.ts"
      },
      "import": "./dist/types.es.js",
      "require": "./dist/types.cjs.js"
    },
    "./types-log": {
      "types": {
        "require": "./dist/log.d.ts",
        "default": "./dist/log.d.ts"
      },
      "import": "./dist/types.es.js",
      "require": "./dist/types.cjs.js"
    },
    "./css": {
      "import": "./dist/plugin.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "path": "^0.12.7",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1",
    "vue-tsc": "^1.0.11"
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.8",
    "lodash-es": "^4.17.21"
  }
}
